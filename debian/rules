#!/usr/bin/make -f

srctree ?= .

build:
	$(MAKE) KERNELRELEASE=5.10.160-rockchip-rk356x ARCH=arm64 	KBUILD_BUILD_VERSION=1.0.4 -f $(srctree)/Makefile

binary-arch:
	$(MAKE) KERNELRELEASE=5.10.160-rockchip-rk356x ARCH=arm64 	KBUILD_BUILD_VERSION=1.0.4 -f $(srctree)/Makefile intdeb-pkg

clean:
	rm -rf debian/*tmp debian/files
	$(MAKE) clean

binary: binary-arch
